class Solution {
public:
    int minimumOperations(vector<int>& nums) {
        int cnt=0;
        for(int i=0;i<nums.size();i=i+3){
            unordered_set<int>st;
            for(int j=i;j<nums.size();j++){
                st.insert(nums[j]);
                
            }
            if((nums.size()-i)!=st.size()){
                cnt++;
            }
            else{
                break;
            }
        }
        return cnt;
    }
};